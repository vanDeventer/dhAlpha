<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.operation" table="operation">
      <id  name="id" type="int" column="operation_id">
        <generator class="native"/>
      </id>
      <many-to-one name="operation_type_id" class="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.operation_type"
            column="operation_type_id" unique="true" not-null="true"
            cascade="all" />
      
      <property name="name" column="name" type="string"/>
      <property name="description" column="description" type="string"/>
    </class>
</hibernate-mapping>
