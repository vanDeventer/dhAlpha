<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.assignment_path" table="assignment_path">
      <id  name="id" type="int" column="assignment_path_id">
        <generator class="native"/>
      </id>
      <many-to-one name="node_id" class="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.node"
            column="node_id" unique="true" not-null="true"
            cascade="all" />
   </class>
</hibernate-mapping>
