<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.arrowhead_models.arrowheadsystem" table="arrowheadsystem">
        <id  name="id" type="int" column="system_id">
        <generator class="native"/>
      </id>
      <property name="IPAddress" column="IPAddress" type="string"/>
      <property name="authenticationInfo" column="authenticationInfo" type="string"/>
      <property name="port" column="port" type="string"/>
      <property name="systemName" column="systemName" type="string"/>
      <many-to-one name="node_id" class="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.node"
            column="node_id" unique="true" not-null="true"
            cascade="all" />
   </class>
</hibernate-mapping>
