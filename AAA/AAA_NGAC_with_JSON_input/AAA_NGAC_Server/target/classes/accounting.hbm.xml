<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.Accounting.accounting_table" table="accounting_table">
		<id name="ID" type="int" column="ID">
			<generator class="increment"/>
		</id>
  	  <property  name="SessionID" type="int" column="SessionID"/>
      <property name="Consumer" column="Consumer" type="string"/>
      <property name="Producer" column="Producer" type="string"/>
      <property name="ServiceName" column="ServiceName" type="string"/>
      <property name="RequestingEntity" column="RequestingEntity" type="string"/>
      <property name="InboundRequests" column="InboundRequests" type="int"/>
      <property name="OutResponses" column="OutResponses" type="int"/>
      <property name="SessionStartTime" column="SessionStartTime" type="long"/>
      <property name="SessionEndTime" column="SessionEndTime" type="long"/>
      <property name="MinRequestSize" column="MinRequestSize" type="int"/>
      <property name="MaxRequestSize" column="MaxRequestSize" type="int"/>
      <property name="IPaddressChange" column="IPaddressChange" type="int"/>
      <property name ="TerminationCause" column= "TerminationCause" type= "int"/>
   </class>
</hibernate-mapping>
