<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.arrowhead_models.arrowheadcloud" table="arrowheadcloud">
   <id  name="id" type="int" column="cloud_id">
        <generator class="native"/>
      </id>
      <property name="cloudName" column="cloudName" type="string"/>
      <property name="authenticationInfo" column="authenticationInfo" type="string"/>
      <property name="operator" column="operator" type="string"/>
   </class>
</hibernate-mapping>
