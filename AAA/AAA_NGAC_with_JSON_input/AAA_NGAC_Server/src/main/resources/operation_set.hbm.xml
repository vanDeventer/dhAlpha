<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.operation_set" table="operation_set">
     
      <id  name="id" type="int" column="set_id">
        <generator class="native"/>
      </id>
      <many-to-one name="operation_id" class="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.operation"
            column="operation_id" unique="true" not-null="true"
            cascade="all" />
      <many-to-one name="node_id" class="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.node"
            column="operation_node_id" unique="true" not-null="true"
            cascade="all"/>

    </class>
      
</hibernate-mapping>
