<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
  <class name="com.aaa_ngac_demo.AAA_NGAC_Server.ngac_database.node" table="node">
            <id  name="id" type="int" column="node_id">
         <generator class="native"/>
      </id>
      <property name="name" column="name" type="string"/>
      <property name="description" column="description" type="string"/>
      <property name="nodetype_id" column="nodetype_id" type="int"/>

   </class>
</hibernate-mapping>
